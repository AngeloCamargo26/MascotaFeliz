@page
@model PracticaVeterinaria.App.Presentacion.Pages.ListVeterinario
@{
}
<h1>Lista de Veterinarios</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Tarjeta Profesional</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Telefono</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var Veterinario in Model.veterinarios)
        {
            <tr scope="row">
                <td>@Veterinario.id</td>
                <td>@Veterinario.tarjetaProfesional</td>
                <td>@Veterinario.nombres</td>
                <td>@Veterinario.apellidos</td>
                <td>@Veterinario.telefono</td>
                <td>
                    <a asp-page="./EditVeterinario" asp-route-id="@Veterinario.id">Editar</a> |
                    <a asp-page="./DeleteVeterinario" asp-route-id="@Veterinario.id">Delete</a> |

                </td>

            </tr>
        }
    </tbody>
</table>

<div class="row">
    <div class="col-md-4">
        <form method="get" action="./AddVeterinario">
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>