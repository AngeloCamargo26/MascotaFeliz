@page
@model PracticaVeterinaria.App.Presentacion.Pages.AddModel
@{
}
<h1>Crear Propietario</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NuevoPropietario.nombres" class="control-label"></label>
                <input asp-for="NuevoPropietario.nombres" class="form-control" />
                <span asp-validation-for="NuevoPropietario.nombres" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NuevoPropietario.apellidos" class="control-label"></label>
                <input asp-for="NuevoPropietario.apellidos" class="form-control" />
                <span asp-validation-for="NuevoPropietario.apellidos" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NuevoPropietario.cedula" class="control-label"></label>
                <input asp-for="NuevoPropietario.cedula" class="form-control" />
                <span asp-validation-for="NuevoPropietario.cedula" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NuevoPropietario.direccion" class="control-label"></label>
                <input asp-for="NuevoPropietario.direccion" class="form-control" />
                <span asp-validation-for="NuevoPropietario.direccion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NuevoPropietario.correoElectronico" class="control-label"></label>
                <input asp-for="NuevoPropietario.correoElectronico" class="form-control" />
                <span asp-validation-for="NuevoPropietario.correoElectronico" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NuevoPropietario.telefono" class="control-label"></label>
                <input asp-for="NuevoPropietario.telefono" class="form-control" />
                <span asp-validation-for="NuevoPropietario.telefono" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>